            {% include 'TemploUserBundle:Security:_modalLogin.html.twig' %}
        {% endif%}       
            
        <div class="container">
        {% block main %}{% endblock %}
	</div>	 
     
	{% include 'TemploBundle:includes:footer.html.twig' %}
    
            
        {% block foot_script_assetic %}
            <script src="{{ asset('assets/js/jquery-1.11.0.min.js') }}"></script>            
